# Server Configuration
PORT=3000
NODE_ENV=development

# Google OAuth 2.0
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# JWT Secret
JWT_SECRET=your_jwt_secret_key_change_in_production

# Session Secret
SESSION_SECRET=your_session_secret_change_in_production

# Encryption Key for PIN Recovery
ENCRYPTION_KEY=your_32_byte_encryption_key_change_in_production

# Blockchain RPC URLs
SEPOLIA_RPC_URL=https://ethereum-sepolia.publicnode.com
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
OPTIMISM_SEPOLIA_RPC_URL=https://sepolia.optimism.io

# Deployer Private Key (for multi-chain deployment)
DEPLOYER_PRIVATE_KEY=your_private_key_for_deployment

# Contract Addresses (will be populated after deployment)
ENTRYPOINT_ADDRESS=
ACCOUNT_FACTORY_ADDRESS=
PAYMASTER_ADDRESS=
TEST_ERC20_ADDRESS=

# Frontend URL (for CORS)
FRONTEND_URL=chrome-extension://your-extension-id
